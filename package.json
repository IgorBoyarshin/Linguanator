{
  "name": "linguanator",
  "version": "1.0.0",
  "description": "The Linguanator application to learn new words",
  "scripts": {
    "deploy": "npm run cache_clear & npm run build:prod",
    "build_f": "npm run cache_clear & npm run rm_dist & npm run build & npm run copy_database",
    "build_f:prod": "npm run cache_clear & npm run rm_dist & npm run build:prod & npm run copy_database",
    "build": "node_modules/.bin/webpack --progress",
    "build:prod": "node_modules/.bin/webpack -p --progress",
    "copy_css": "cp src/styles.css dist/styles.css",
    "copy_database": "cp -rf database dist/database",
    "cache_clear": "npm cache clear",
    "serve": "webpack-dev-server --inline --hot --progress",
    "webp": "node_modules/.bin/webpack --progress",
    "build_tsc": "tsc --rootDir src --outDir dist",
    "rm_dist": "rd dist /s /q",
    "postinstall": "typings install"
  },
  "keywords": [],
  "author": "Igor Boyarshin",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "reflect-metadata": "0.1.9",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/node": "^6.0.58",
    "angular2-template-loader": "^0.6.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.26.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "typings": "2.1.0",
    "webpack": "^1.14.0"
  },
  "repository": {}
}
